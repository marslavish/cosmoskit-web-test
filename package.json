{
  "version": "0.0.0-alpha.10",
  "name": "@cosmology/cosmoskit.com",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --resolve-plugins-relative-to . --ext pages/**/* components/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix --resolve-plugins-relative-to . --ext pages/**/* components/**/*.{ts,tsx}",
    "format": "yarn lint:fix && yarn style",
    "style": "prettier --write '**/*.{ts,tsx}' --config ./.prettierrc",
    "export": "next build && next export",
    "sync": "AWS_PROFILE=webinc aws s3 sync out/ s3://cosmoskit.com",
    "sync:stage": "AWS_PROFILE=webinc aws s3 sync out/ s3://stage.cosmoskit.com",
    "invalidate": "AWS_PROFILE=webinc aws cloudfront create-invalidation --distribution-id E11QJSUOVT2WF0 --paths \"/*\"",
    "invalidate:stage": "AWS_PROFILE=webinc aws cloudfront create-invalidation --distribution-id EVVMCJ8N7Z0QP --paths \"/*\"",
    "deploy:all": "npm run export && npm run sync && S3_BUCKET=cosmoskit.com ./seo/prepare.sh",
    "deploy:stage": "npm run export && npm run sync:stage && S3_BUCKET=stage.cosmoskit.com ./seo/prepare.sh",
    "postinstall": "node ./scripts/copy-assets-to-public-dir.js"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.19",
    "@chakra-ui/react": "2.7.1",
    "@cosmjs/stargate": "0.28.4",
    "@cosmology/core": "^0.4.0",
    "@cosmology/cosmos-registry": "^0.0.13",
    "@cosmology/react": "^0.0.10",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@interchain-ui/react": "^1.1.11",
    "@keplr-wallet/cosmos": "^0.10.3",
    "@keplr-wallet/types": "0.10.3",
    "@keplr-wallet/unit": "0.10.3",
    "add": "2.0.6",
    "chain-registry": "1.2.0",
    "chakra-react-select": "4.1.4",
    "fast-fuzzy": "^1.12.0",
    "framer-motion": "10.12.18",
    "fuzzy": "0.1.3",
    "mobx": "6.6.1",
    "mobx-react-lite": "3.4.0",
    "mobx-utils": "^6.0.3",
    "next": "^12.3.4",
    "next-seo": "5.5.0",
    "prop-types": "^15.7.2",
    "react": "18.2.0",
    "react-code-blocks": "0.1.0",
    "react-dom": "18.2.0",
    "react-frame-component": "5.2.6",
    "react-player": "2.10.1",
    "yarn": "1.22.19",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.5",
    "@babel/eslint-parser": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-export-default-from": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@next/eslint-plugin-next": "^12.3.4",
    "@types/react": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
